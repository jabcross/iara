// RUN: export SCHEDULER_MODE=$SCHEDULER_MODE
// RUN: pwd >&2
// RUN: build-single-test.sh .
// RUN: pwd >&2
// RUN: ls build >&2
// RUN: ./build/a.out > ./build/output.txt
// RUN: FileCheck %s < ./build/output.txt

iara.actor @run {
  %e_0_0_0,      %e_0_1_0,      %e_0_2_0,      %e_1_0_0,      %e_1_1_0,
  %e_1_2_0,      %e_2_0_0,      %e_2_1_0,      %e_2_2_0
  = iara.node @kernel_split out
  f64, f64, f64, f64, f64, f64, f64, f64, f64
  %e_0_0_1 = iara.node @kernel_potrf inout %e_0_0_0 : f64 out f64
  %e_0_1_1 = iara.node @kernel_trsm in %e_0_0_1 : f64 inout %e_0_1_0 : f64 out f64
  %e_0_2_1 = iara.node @kernel_trsm in %e_0_0_1 : f64 inout %e_0_2_0 : f64 out f64
  %e_1_1_1 = iara.node @kernel_syrk in %e_0_1_1 : f64 inout %e_1_1_0 : f64 out f64
  %e_1_2_1 = iara.node @kernel_gemm in ( %e_0_2_1: f64,  %e_0_1_1 : f64 ) inout %e_1_2_0 : f64 out f64
  %e_2_2_1 = iara.node @kernel_syrk in %e_0_2_1 : f64 inout %e_2_2_0 : f64 out f64
  %e_1_1_2 = iara.node @kernel_potrf inout %e_1_1_1 : f64 out f64
  %e_1_2_2 = iara.node @kernel_trsm in %e_1_1_2 : f64 inout %e_1_2_1 : f64 out f64
  %e_2_2_2 = iara.node @kernel_syrk in %e_1_2_2 : f64 inout %e_2_2_1 : f64 out f64
  %e_2_2_3 = iara.node @kernel_potrf inout %e_2_2_2 : f64 out f64

  iara.node @kernel_join
    in
      %e_0_0_1 : f64,
      %e_0_1_1 : f64,
      %e_0_2_1 : f64,
      %e_1_0_0 : f64,
      %e_1_1_2 : f64,
      %e_1_2_2 : f64,
      %e_2_0_0 : f64,
      %e_2_1_0 : f64,
      %e_2_2_3 : f64
}