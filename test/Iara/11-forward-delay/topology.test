// forward delay

// Two nodes

iara.actor @a {
    iara.out : tensor<2xi32>
}

iara.actor @b {
    iara.in : tensor<3xi32>
}

iara.actor @run  {
  %0 = iara.node @a out: tensor<2xi32>
  %1 = iara.edge %0 : tensor<2xi32> -> tensor<3xi32> {delay = array<i32: 100, 101, 102, 103, 104>}
  iara.node @b in %1: tensor<3xi32>
} { }

// First iteration consumes delay and 1 value generated by A
// CHECK: {{^ *}}100 101 102 103 104 0{{ *$}}

// Second picks up after
// CHECK: {{^ *}}1 2 3 4 5 6{{ *$}}